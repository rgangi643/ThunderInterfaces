{
  "$schema": "interface.schema.json",
  "jsonrpc": "2.0",
  "info": {
    "title": "Airplay API",
    "class": "AirplayJSONRPC",
    "description": "Airplay JSON-RPC interface"
  },
  "common": {
	      "$ref": "common.json"
	    },
  "include": {
        "statecontrol": {
          "$ref": "StateControl.json#"
        }
  },
  "methods": {
         "startAirplayApp": {
      "summary": "Does Airplay system command",
      "params": {
        "type": "object",
        "properties": {
          "command": {
            "description": "Command to send to Airplay",
            "type": "string",
            "example": "config"
          }
        }
      },
      "result": {
        "type": "number"
      },
      "errors": [
        {
          "description": "Airplay is unavailable",
          "$ref": "#/common/errors/unavailable"
        },
        {
          "description": "Command is not set",
          "$ref": "#/common/errors/unknownkey"
        }        
      ]
    }
  }
 }
 
